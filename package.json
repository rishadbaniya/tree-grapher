    "react-vextensions": "1.0.23",
    "tree-grapher": "file:.",
    "ts-loader": "9.2.6",
    "typescript": "5.3.3",
    "ui-debug-kit": "git+https://github.com/rishadbaniya/ui-debug-kit.git#2a68c9fd435fbe67c160ba473b594165ab3c85cb",
    "webpack": "5.88.2",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@types/d3-hierarchy": "^3.0.2",
